                "view": "~/App_Plugins/OpeningHours/views/openingHours.weekday.html",
				"valueType": "JSON"
            }
        },
		{            
            "alias": "HolidayOpeningHours",
            "name": "Holiday Opening Hours",
            "icon": "icon-calendar-alt",
            "group": "Opening Hours", 
            "editor": {
                "view": "~/App_Plugins/OpeningHours/views/openingHours.holiday.html",
