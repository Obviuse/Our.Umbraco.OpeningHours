        {            
            "alias": "OpeningHours",
            "name": " Opening Hours",
            "icon": "icon-calendar-alt",
            "group": "Opening Hours",           
            "editor": {
                "view": "~/App_Plugins/OpeningHours/views/openingHours.html",
				"valueType": "JSON"
            },
			"prevalues": {
				"fields": [
					{
						"label": "Require Holiday Dates",
